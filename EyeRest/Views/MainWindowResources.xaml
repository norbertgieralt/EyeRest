<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:vm="clr-namespace:EyeRest.ViewModels"
                    xmlns:v="clr-namespace:EyeRest.Views"
                    >
    <DataTemplate  DataType="{x:Type vm:ClockScreenViewModel}" >
        <v:ClockScreenView/>
    </DataTemplate>
    <DataTemplate  DataType="{x:Type vm:SettingsViewModel}" >
        <v:SettingsView/>
    </DataTemplate>
    <DataTemplate  DataType="{x:Type vm:QuitViewModel}" >
        <v:QuitView/>
    </DataTemplate>
    <DataTemplate  DataType="{x:Type vm:PictureViewModel}" >
        <v:PictureView/>
    </DataTemplate>
</ResourceDictionary>